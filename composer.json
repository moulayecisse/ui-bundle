{
    "name": "cisse/ui-bundle",
    "description": "A Symfony UI bundle containing reusable anonymous Twig components styled with TailwindCSS",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["symfony", "symfony-ux", "twig", "tailwind", "ui", "components"],
    "homepage": "https://symfony.com",
    "authors": [
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/config": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/framework-bundle": "^7.3",
        "symfony/http-kernel": "^7.3",
        "symfony/string": "^7.3",
        "symfony/twig-bundle": "^7.3",
        "symfony/ux-icons": "^2.32",
        "symfony/ux-twig-component": "^2.32",
        "tales-from-a-dev/twig-tailwind-extra": "^1.0",
        "twig/twig": "^3.0",
        "symfony/uid": "^7.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^7.3",
        "symfony/var-dumper": "^7.3"
    },
    "suggest": {
        "symfonycasts/tailwind-bundle": "For TailwindCSS integration in Symfony"
    },
    "autoload": {
        "psr-4": {
            "Cisse\\Bundle\\Ui\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cisse\\Bundle\\Ui\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "thanks": {
            "name": "symfony/ux",
            "url": "https://github.com/symfony/ux"
        }
    },
    "minimum-stability": "dev"
}
